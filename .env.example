# Copy this file to .env and fill in values. Never commit your real .env.

# MongoDB Atlas connection string (use SRV)
# Example: mongodb+srv://<username>:<password>@cluster0.xxxxxx.mongodb.net/?appName=Cluster0
MONGODB_URI=

# Database and collection names
MONGODB_DB=labyrinth
MONGODB_COLLECTION=player_saves

# Optional: networking/runtime
#HOST=0.0.0.0
#PORT=5000
#FLASK_DEBUG=false
#CORS_ALLOWED_ORIGINS=*
#FORCE_SECURE_COOKIES=false

# Optional: Flask secret key (used for signing cookies/sessions)
#SECRET_KEY=change-me-in-dev
